{
    "version": "0.2.0",
    "configurations": [

        {
            "type": "gdb",
            "request": "launch",
            "name": "Launch Program",
            "target": "${workspaceRoot}/src/build/unp",
            "cwd": "${workspaceRoot}/src/build"
        },
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/src/build/unp",
            //"args": ["193.228.143.22"],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}/src/build",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}